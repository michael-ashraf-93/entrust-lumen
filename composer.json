    "keywords": ["lumen","illuminate","auth","roles","acl","permission"],
    "license": "MIT",
    "authors": [
        {
            "name": "Proshore",
            "email": "babishshrestha@gmail.com"
        },
        {
            "name": "Saroj",
            "email": "saroj@proshore.eu"
        }
    ],
    "require": {
        "php": "^7.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3.1",
        "illuminate/database": "~5.0",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/commands"
